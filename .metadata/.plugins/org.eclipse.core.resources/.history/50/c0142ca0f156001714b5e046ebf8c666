# -*- coding:utf-8 -*-  
from urllib.request import urlopen
from urllib.request import Request
from urllib import parse


req = request.Request("http://www.thsrc.com.tw/tw/TimeTable/SearchResult")
req.add_header("User-Agent", "Mozilla/5.0 (Windows NT 6.3; WOW64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/45.0.2454.101 Safari/537.36")

PostData = parse.urlencode([
    (),
    ()
    ])

resp = request.urlopen(req,data =PostData.encode(encoding='utf_8', errors='strict') )
print(resp.read().decode('utf-8'))
